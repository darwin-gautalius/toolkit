{
  "name": "weekly-report",
  "version": "1.0.0",
  "description": "Generate weekly report in Notion from Slack's Daily Channel",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "maas": "dotenvx run -- tsx src/maas.ts",
    "generate": "dotenvx run -- tsx src/index.ts",
    "daily": "dotenvx run -- tsx src/notion-daily.ts",
    "tag": "dotenvx run -- tsx src/notion-tag.ts",
    "weekly": "dotenvx run -- tsx src/notion-weekly.ts",
    "clear": "cp src/task-template.txt src/tasks.txt"
  },
  "author": "Darwin",
  "license": "ISC",
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "@slack/web-api": "^6.3.0",
    "dayjs": "^1.10.6",
    "dotenv": "16.4.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.60.0",
    "papaparse": "^5.4.1",
    "tsx": "^4.19.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@types/papaparse": "^5.3.14",
    "typescript": "5.6.2"
  }
}